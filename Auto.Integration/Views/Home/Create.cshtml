@model Auto.Integration.Models.AutoBrand

@{
    ViewBag.Title = "Создать бренд";
}

<form asp-action="Create" asp-controller="Home" method="post" asp-antiforgery="true" novalidate>

    <div class="form-group row">
        <div class="col-md-10">
            <label asp-for="Name"></label>
            <input asp-for="Name" class="form-control" required>
        </div>
    </div>
    
    <div class="col form-group mt-5">
        <button type="submit" class="btn btn-primary ml-2">Сохранить</button>
    </div>
</form>